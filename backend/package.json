{
  "name": "hexastats",
  "version": "0.0.1",
  "description": "hexastats backend",
  "author": "dawichi",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "compodoc": "compodoc -p tsconfig.json --port 9001 --includes docs -w -s",
    "docs:cov": "compodoc -p tsconfig.json --coverageTest 80",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "format:augments": "ts-node src/scripts/format_augments.ts"
  },
  "dependencies": {
    "@nestjs/axios": "3.0.1",
    "@nestjs/common": "10.2.10",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.10",
    "@nestjs/platform-express": "10.2.10",
    "@nestjs/swagger": "7.1.16",
    "@upstash/redis": "1.25.1",
    "axios": "1.6.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "swagger-ui-express": "5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.23",
    "@nestjs/cli": "10.2.1",
    "@nestjs/schematics": "10.0.3",
    "@nestjs/testing": "10.2.10",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.10",
    "@types/node": "20.10.2",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "6.13.11",
    "@typescript-eslint/parser": "6.13.1",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "jest": "29.7.0",
    "prettier": "3.1.0",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
